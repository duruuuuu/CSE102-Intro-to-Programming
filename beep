#include <stdio.h>

#define MAX 100

int main()
{
    FILE *input;
    FILE *output;

    input = fopen("input.txt", "r");
    output = fopen("output.txt", "w");

    int num, index = 0;
    int numArr[MAX], countArr[MAX];

    fscanf(input, "%d", &numArr[index]);
    countArr[index] = 1;

    while (fscanf(input, "%d", &num) != EOF)
    {
        int i, flag = 0;
        for (i = 0; i < index; i++)
        {
            if (numArr[i] == num)
            {
                flag = 1;
                break;
            }
        }

        if (flag == 0)
        {
            // index++;
            numArr[++index] = num;
            countArr[index] = 1;
            printf("NUM : %d\n", num);
        }

        else if (flag == 1)
            countArr[i]++;

        printf("%d : %d\n", numArr[index], countArr[index]);
    }
    /*
        for (int x = 0; x < index; x++)
        {
            for (int y = x + 1; y < index; y++)
            {
                // sorting both arrays by count
                if (countArr[x] > countArr[y])
                {
                    int tempC = countArr[x];
                    int tempN = numArr[x];

                    countArr[x] = countArr[y];
                    numArr[x] = numArr[y];

                    countArr[y] = tempC;
                    numArr[y] = tempN;
                }

                // sorting numbers array by value if count is the same
                if (numArr[x] > numArr[y] && countArr[x] == countArr[y])
                {
                    int tempN = numArr[x];
                    numArr[x] = numArr[y];
                    numArr[y] = tempN;
                }
            }
        }
    for (int k = 0; k < index; k++)
        printf("%d : %d\n", numArr[k], countArr[k]);*/
}